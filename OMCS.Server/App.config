<?xml version="1.0"?>
<configuration>
  <appSettings>
    <!--OMCS授权用户的账号-->
    <add key="AuthorizedUser" value="FreeUser"/>
    <!--OMCS授权用户的密码-->
    <add key="AuthorizedPassword" value=""/>

    <!--服务器监听的端口-->
    <add key="Port" value="9900"/>
    <!--是否记录安全日志文件 Omcs.txt-->
    <add key="SecurityLogEnabled" value="false"/>
    <!--UserID的最大长度-->
    <add key="MaxLengthOfUserID" value="11"/>

    <!--摄像头采集视频的帧率-->
    <add key="CameraFramerate" value="6"/>
    <!--桌面帧率-->
    <add key="DesktopFramerate" value="3"/>   
  </appSettings>
</configuration>
